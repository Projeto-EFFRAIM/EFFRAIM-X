{
	"manifest_version": 3,
	"name": "EFFRAIM - X",
	"version": "0.9.1.1",
	"description": "Simplifica tarefas no Eproc: integração, navegação, relatórios e mais.",
	"icons": {
		"16": "assets/icones/icone16.png",
		"48": "assets/icones/icone48.png",
		"128": "assets/icones/icone128.png"
	},
	"permissions": [
		"storage",
		"tabs"
	],
	"host_permissions": [
		"https://eproc.jfrj.jus.br/*",
		"https://sisbajud.cnj.jus.br/*",
		"https://sso.cloud.pje.jus.br/auth/*",
		"https://renajud.pdpj.jus.br/*",
		"https://renajud.denatran.serpro.gov.br/*",
		"https://portaldeestatisticas.trf2.jus.br/*"
		],
	"options_page": "preferencias.html",
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		{
		"matches": ["https://eproc.jfrj.jus.br/*"],
		"js": ["content/eproc_init.js"],
		"run_at": "document_idle"
		},
		{
		"matches": [
		"https://sisbajud.cnj.jus.br/*",
		"https://sso.cloud.pje.jus.br/*"
		],
		"js": ["content/sisbajud_init.js"],
		"run_at": "document_idle",
		"all_frames": true
		},
		{
		"matches": [
		"https://renajud.pdpj.jus.br/*",
		"https://renajud.denatran.serpro.gov.br/*"
		],
		"js": ["content/renajud_init.js"],
		"run_at": "document_idle",
		"all_frames": true
		},
		{
		"matches": [
		"https://portaldeestatisticas.trf2.jus.br/*"
		],
		"js": ["content/corregedoria_init.js"],
		"run_at": "document_idle",
		"all_frames": true
		},
		{
		"matches": [
		"https://sso.cloud.pje.jus.br/auth/*"
		],
		"js": ["content/sso_pdpj.js"],
		"run_at": "document_idle",
		"all_frames": true
		}
	],
	"action": {
		"default_icon": {
		"16": "assets/icones/icone16.png",
		"48": "assets/icones/icone48.png",
		"128": "assets/icones/icone128.png"
		},
		"default_title": "EFFRAIM"
	},
	"web_accessible_resources": [
		{
			"resources": [
			"assets/css/*.css",
			"assets/icones/*.png",
			"configuracoes.json",
			"modules/*",
			"ajuda/*",
			"preferencias.html",
			"funcoes.js"
			],
			"matches": [
			"https://eproc.jfrj.jus.br/*",
			"https://sisbajud.cnj.jus.br/*",
			"https://sso.cloud.pje.jus.br/*",
			"https://renajud.pdpj.jus.br/*",
			"https://renajud.denatran.serpro.gov.br/*",
			"https://portaldeestatisticas.trf2.jus.br/*"
			]
		}
	],
	"key": ""
}
